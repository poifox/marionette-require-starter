{
	"sass": {
		"dev": {
			"options": {
				"cache": false,
				"trace": true,
				"no-cache": true,
				"style": "nested"
			},
			"files": {
				"dist/css/styles.css": "css/styles.scss"
			}
		},
		"dist": {
			"options": {
				"trace": true,
				"no-cache": true,
				"unixNewlines": true,
				"style": "compressed"
			},
			"files": {
				"dist/css/styles.min.css": "css/styles.scss"
			}
		}
	}
}
